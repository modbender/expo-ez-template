{
  "name": "{{APP_SLUG}}",
  "main": "expo-router/entry",
  "version": "0.1.0",
  "scripts": {
    "start": "expo start --tunnel",
    "android": "REACT_NATIVE_PACKAGER_HOSTNAME=localhost expo run:android",
    "android:nobundler": "expo run:android --no-bundler",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll=false",
    "test:watch": "jest",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "fix": "bunx expo install --fix",
    "prebuild": "expo prebuild",
    "apk": "NODE_ENV=production eas build -p android --profile preview --local --output ./build-$(node -p \"require('./package.json').version\").apk",
    "aab": "NODE_ENV=production eas build -p android --profile production --local --output ./build-$(node -p \"require('./package.json').version\").aab",
    "submit": "eas submit -p android --profile production",
    "prepare": "husky",
    "release": "release-it",
    "release:patch": "release-it patch",
    "release:minor": "release-it minor",
    "release:major": "release-it major"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.31",
    "expo-build-properties": "~1.0.10",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.10",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.21",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@release-it/conventional-changelog": "^10.0.4",
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.16",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.3",
    "release-it": "^19.2.1",
    "typescript": "~5.9.2"
  },
  "private": true,
  "packageManager": "bun@1.2.15",
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md}": "prettier --write"
  }
}